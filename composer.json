{
    "name": "drawmyattention/laravel-make-resource",
    "description": "Generate a model, migration, controller, routes and factory",
    "keywords": ["laravel", "generators"],
    "license": "MIT",
    "authors": [
      {
        "name": "Amo Chohan",
        "email": "amo.chohan@gmail.com"
      }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.2.*",
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "DrawMyAttention\\ResourceGenerator\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "DrawMyAttention\\ResourceGenerator\\": ["src/", "tests/"],
        "App\\": "tests/generated/app/"
      }
    }
}
